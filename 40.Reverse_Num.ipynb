   "source": [
    " Find the reverse of a number provided by the user(any number of digit)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "987654321\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter the number : \"))\n",
    "\n",
    "rev = 0\n",
    "\n",
    "while (num > 0):\n",
    "    a = num % 10\n",
    "    rev = rev * 10 + a\n",
    "    num = num // 10\n",
    "\n",
    "print(rev)"
   ]
