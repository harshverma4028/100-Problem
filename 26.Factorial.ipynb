   "source": [
    "no = int(input(\"Enter the no : \"))\n",
    "\n",
    "fact = 1\n",
    "i = 1\n",
    "if no == 0 | 1:\n",
    "    print(\"The factorial of 0 and 1 is 1 \")\n",
    "else:\n",
    "    while i <= no:\n",
    "        fact *=i\n",
    "        i+=1\n",
    "print(fact)        \n"
