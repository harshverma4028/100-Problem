   ],
   "source": [
    "a = input(\"Enter the string you want to count : \")\n",
    "vowel = 'aeiou'\n",
    "count = 0\n",
    "\n",
    "for i in a:\n",
    "    if i in vowel:\n",
    "        count += 1\n",
    "\n",
    "print(count)"
