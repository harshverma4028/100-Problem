   "source": [
    "Write a python program to reverse a list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "the original list is [12, 13, 14, 15, 16, 17, 18, 19, 20]\n",
      "the reversed list is [20, 19, 18, 17, 16, 15, 14, 13, 12]\n"
     ]
    }
   ],
   "source": [
    "list = [12,13,14,15,16,17,18,19,20]\n",
    "listr = []\n",
    "\n",
    "for i in range(len(list)-1,-1,-1):\n",
    "    listr.append(list[i])\n",
    "\n",
    "print(f\"the original list is {list}\")    \n",
    "print(f\"the reversed list is {listr}\")"
   ]
