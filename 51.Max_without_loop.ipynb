   "source": [
    "Write a python program to find the max item from a list without using the max function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "the max is 85\n"
     ]
    }
   ],
   "source": [
    "List = [10,12,2,5,4,7,23,26,56,85,54,57,6,25,11]\n",
    "\n",
    "max = 1\n",
    "\n",
    "for i in range(0,len(List)):\n",
    "\n",
    "        if max <= List[i]:\n",
    "            max = List[i]\n",
    "\n",
    "print(f\"the max is {max}\")    \n"
   ]
